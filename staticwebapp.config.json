{
  "routes": [
    {
      "route": "/*",
      "serve": "/index.html",
      "statusCode": 200
    }
  ],
  "platformErrorOverrides": [
    { "errorType": "NotFound", "serve": "/not_found" },
    {
      "errorType": "Unauthenticated",
      "statusCode": "302",
      "serve": "/auth/login"
    }
  ],
  "mimeTypes": {
    "json": "application/json"
  }
}